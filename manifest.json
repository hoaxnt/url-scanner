{
   "action": {
      "default_icon": "icon.png",
      "default_popup": "/popup.html"
   },
   "background": {
      "service_worker": "/js/service.js",
      "type": "module"
   },
   "content_scripts": [
      {
         "js": [ "/js/jquery.min.js", "/js/word.js", "/js/content.js" ],
         "matches": [ "*://*.bing.com/*" ],
         "run_at": "document_start"
      },
      {
         "js": [ "/js/rewards.js" ],
         "matches": [ "https://www.bing.com/rewards/*" ],
         "run_at": "document_start"
      }
   ],
