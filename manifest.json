   "host_permissions": [ "*://*.bing.com/*" ],
   "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Rewards Search Automator",
   "permissions": [ "alarms", "browsingData", "debugger", "storage", "tabs", "notifications" ],
